  "scripts": {
    "build:server":
      "NODE_ENV=production REACT_APP_STAGE=production node scripts/build-server.js",
    "run:server":
      "NODE_ENV=production REACT_APP_STAGE=production node build/server/static/js/main.js",
    "start":
      "REACT_APP_STAGE=production node -r dotenv/config scripts/start.js",
    "start:desktop":
      "REACT_APP_STAGE=production node -r dotenv/config scripts/start.js",
    "stage": " REACT_APP_STAGE=stage node scripts/build.js",
    "tmpprod": " REACT_APP_STAGE=tmpprod node scripts/build.js",
    "local": " REACT_APP_STAGE=local node scripts/build.js",
    "dev": " REACT_APP_STAGE=devxelp node scripts/build.js",
    "uat2": " REACT_APP_STAGE=uat2 node scripts/build.js",
    "build": "REACT_APP_STAGE=production node scripts/build.js",
    "tmpprod-desktop": "REACT_APP_STAGE=tmpprod node scripts/build.js",
    "p2": "REACT_APP_STAGE=p2 node scripts/build.js",
    "uat": "REACT_APP_STAGE=uat node scripts/build.js",
    "uat5": "REACT_APP_STAGE=uat5 node scripts/build.js",
    "uat6": "REACT_APP_STAGE=uat6 node scripts/build.js",
    "e2e": "REACT_APP_STAGE=e2e node scripts/build.js",
    "e2e1": "REACT_APP_STAGE=e2e1 node scripts/build.js",
    "uat6tcs": "REACT_APP_STAGE=uat6tcs node scripts/build.js",
    "qa2": "REACT_APP_STAGE=qa2 node scripts/build.js",
    "pt": "REACT_APP_STAGE=pt node scripts/build.js",
    "production":
      "GENERATE_SOURCEMAP=false REACT_APP_STAGE=production node scripts/build.js",
    "production-cdn":
      "GENERATE_SOURCEMAP=false REACT_APP_STAGE=production PUBLIC_URL=https://pwassets.tatacliq.com node scripts/build.js",
    "p2-production-cdn":
      "REACT_APP_STAGE=p2 PUBLIC_URL=https://pwassetsp2.tatacliq.com node scripts/build.js",
    "production-cdn-desktop":
      "GENERATE_SOURCEMAP=false REACT_APP_STAGE=production PUBLIC_URL=https://pwassets.tatacliq.com/desktop node scripts/build.js",
    "test": "cross-env node scripts/test.js --env=jsdom",
    "lint": "eslint 'src/**/*.js'",
    "pre-build":
      "yarn run tmpprod-desktop  && sudo pm2 startOrRestart ecosystem.config.js",
    "deploy": "pm2 deploy ecosystem.config.js production --force",
    "precommit": "lint-staged",
    "ssr": "NODE_ENV=DFGDFG server/bootstrap.js"
  },
  "lint-staged": {
    "*.js": ["eslint", "prettier --parser flow --write", "git add"]
  },
  "devDependencies": {
    "brotli-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^1.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-recommended": "^2.0.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-css-chunks-webpack-plugin": "^2.0.19",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "pm2": "^3.5.1",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^1.9.2",
    "redux-mock-store": "^1.4.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)":
        "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": ["react-app"]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
