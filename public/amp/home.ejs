<body>
<div>
  <div class="hide-content" on="tap:autoSearch.submit" role="search-submit" tabindex="1"
    [class]="searchClicked ? 'page-search-box-ic' : 'hide-content'"></div>
  <div class="hide-content" [class]="searchClicked ? 'page-search-box' : 'hide-content'">
    <form id="autoSearch" method="get" action="/amp/search/?searchCategory=all" target="_top">
      <input id="searchInput" placeholder="What are you looking for?" class="search-input" name="text" type="text" />
    </form>
  </div>

  <div id="searchClose" class="hide-content" [class]="searchClicked ? 'page-search-close' : 'hide-content'"
  on="tap:AMP.setState({searchClicked : false}),searchClose.hide,autoSearch.clear" role="search-close" tabindex="0"></div>

  <div class="page-left-logo" on="tap:AMP.goBack" role="go-back" tabindex="0"></div>
  <div class="page-right-search" role="search-open" tabindex="0"
    on="tap:AMP.setState({searchClicked : true}),searchClose.show,searchInput.focus"></div>
  <div class="page-header">
    <span>Welcome to Tata CLiQ</span>
  </div>

  <div class="header-backspace"> </div>

  <amp-list src="https://www.tatacliq.com/marketplacewebservices/v2/mpl/cms/defaultpage?pageId=defaulthomepage" width="auto" height="600" layout="fixed-height">
  <!-- <amp-list src="https://api.myjson.com/bins/1bxpji" width="auto" height="600" layout="fixed-height"> -->
    <template type="amp-mustache">

      {{#heroBannerComponent}}
        <!-- NOTE: Hero Banner -->
        <amp-carousel class="hero-banner-component" width="380" height="220" layout="responsive" type="slides" loop>
          {{#heroBannerComponent.items}}
          <div class="hero-banner">
            <a class="no-select" href="{{webURL}}">
              <div class="hero-content">
                <div>{{title}}</div>
                <div>{{subTitle}}</div>
              </div>
              <amp-img class="contain" src="{{imageURL}}" width="340" height="220" layout="responsive"></amp-img>
            </a>
          </div>
          {{/heroBannerComponent.items}}
        </amp-carousel>
      {{/heroBannerComponent}}

      {{#offersComponent}}
        <!-- NOTE: Offer Component -->
        <div class="offer-component top-space">
          <div class="component-header">
            {{offersComponent.title}}
          </div>
          <amp-carousel class="carousel-component" height="320" layout="fixed-height" type="carousel">
            {{#offersComponent.items}}
            <a class="no-select" href="{{webURL}}">
              <div class="offer-item">
                <!-- <a href="{{webURL}}"> -->
                  {{#discountText}}
                  <div class="offer-badge">
                    <div>{{discountText}}</div>
                  </div>
                  {{/discountText}}
                  <div class="offer-content">
                    <div>{{title}}</div>
                    <div><div class="shop-now">{{btnText}}</div></div>
                  </div>
                  <amp-img class="contain" src="{{imageURL}}" width="310" height="310" layout="flex-item"></amp-img>
                <!-- </a> -->
              </div>
            </a>
            {{/offersComponent.items}}
          </amp-carousel>
        </div>
      {{/offersComponent}}

      {{#singleBannerComponent}}
        <!-- NOTE: single banner -->
        <div class="single-component top-space">
          <div class="single-component-header">
            {{singleBannerComponent.title}}
          </div>
          <amp-carousel class="carousel-component" width="320" height="320" layout="responsive" type="slides">
            {{#singleBannerComponent.items}}
            <a class="no-select" href="{{webURL}}">
              <div class="offer-item">
                <!-- <a href="{{webURL}}"> -->
                  {{#discountText}}
                  <div class="offer-badge">
                    <div>{{discountText}}</div>
                  </div>
                  {{/discountText}}
                  <div class="offer-content">
                    <div>{{title}}</div>
                    <div><div class="shop-now">{{btnText}}</div></div>
                  </div>
                  <amp-img class="contain" src="{{imageURL}}" width="320" height="320" layout="responsive"></amp-img>
                <!-- </a> -->
              </div>
            </a>
            {{/singleBannerComponent.items}}
          </amp-carousel>
        </div>
      {{/singleBannerComponent}}

      {{#curatedListingStripComponent}}
      <!-- NOTE: Listing Strip Component -->
      <div class="listing-component">
        <a class="no-select" href="{{curatedListingStripComponent.webURL}}">
          <table class="listing-strip" bgcolor="{{curatedListingStripComponent.startHexCode}}">
            <tr><td>{{curatedListingStripComponent.title}} <span></span></td></tr>
          </table>
        </a>
      </div>
      {{/curatedListingStripComponent}}

      {{#themeOffersComponent}}
      <!-- NOTE: Theme Offers Component -->
      <table class="theme-component top-space" bgcolor="{{themeOffersComponent.backgroundHexCode}}">
        <tbody><tr><td>
          <div>
            <div class="theme-component-header">
              {{themeOffersComponent.title}}
            </div>
            <amp-carousel class="carousel-component" height="320" layout="fixed-height" type="carousel">
              {{#themeOffersComponent.offers}}
                <div>
                  <a class="no-select" href="{{webURL}}">
                    <div class="theme-content">
                      <amp-img class="contain" src="{{imageURL}}" width="140" height="190" layout="flex-item"></amp-img>
                      <div class="theme-desc">
                        <div>{{title}}</div>
                        <div>{{description}}</div>
                      </div>
                    </div>
                  </a>
                </div>
              {{/themeOffersComponent.offers}}
            </amp-carousel>
            <div class="theme-component-footer">
              <a class="no-select" href="{{webURL}}"><div class="shop-now">{{btnText}}</div></a>
            </div>
          </div>
        </td></tr></tbody>
      </table>
      {{/themeOffersComponent}}
    </template>
  </amp-list>

  <div class="footer-backspace"> </div>
  <div class="footer-menu">
    <a class="no-select" href="https://tatacliq.com"><div class="footer-item footer-first">
      <div class="home-icon"><span></span></div>
      <div>Home</div>
    </div></a>
    <a class="no-select" href="https://www.tatacliq.com/categories"><div class="footer-item">
      <div class="category-icon"><span></span></div>
      <div>Categories</div>
    </div></a>
    <a class="no-select" href="https://www.tatacliq.com/brands"><div class="footer-item">
      <div class="brand-icon"><span></span></div>
      <div>Brands</div>
    </div></a>
    <a class="no-select" href="https://tatacliq.com/my-account"><div class="footer-item">
      <div class="account-icon"><span></span></div>
      <div>My Account</div>
    </div></a>
    <a class="no-select" href="https://tatacliq.com/cart"><div class="footer-item">
      <div class="bag-icon"><span></span></div>
      <div>My Bag</div>
    </div></a>
  </div>
</div>
</body>

</html>
