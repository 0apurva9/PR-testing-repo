              <div class="pd-features-desc">
                <ul>
                  {{#returnAndRefund}}
                    <li>{{refundReturnItem}}</li>
                  {{/returnAndRefund}}
                </ul>
              </div>
            </section>
            {{/returnAndRefund.0}}
            <section>
              <h4 class="no-select">Know More<span class="pd-features-arrow"></span></h4>
              <div class="pd-features-desc">
                <ul>
                  {{#knowMore}}
                    <li>{{knowMoreItem}}</li>
                  {{/knowMore}}
                </ul>
              </div>
            </section>
            <section>
              <h4 class="no-select">Brand Info<span class="pd-features-arrow"></span></h4>
              <div class="pd-features-desc">
                {{brandInfo}}
              </div>
            </section>
          </amp-accordion>

          <div class="hide-content" [class]="'{{rootCategory}}' != 'Watches' ? 'show-content pd-features' : 'hide-content'">
            <amp-accordion id="productMoreClassifications" animate>
              {{#classifications}}
              <section>
                <h4 class="no-select">{{groupName}}<span class="pd-features-arrow"></span></h4>
                <div class="pd-features-desc">
                  {{#specifications}}
                  <div>
                    <div class="pd-feature-details">{{key}}</div>
                    <div class="pd-features-desc">
                      {{value}}
                    </div>
                  </div>
                  {{/specifications}}
                </div>
              </section>
              {{/classifications}}
            </amp-accordion>
          </div>

          <div class="hide-content" [class]="'{{rootCategory}}' == 'Watches' ? 'show-content pd-features' : 'hide-content'">
            <amp-accordion id="productMoreClassificationsWatches" animate>
              <section>
                <h4 class="no-select">Product Details<span class="pd-features-arrow"></span></h4>
                <div>
                  {{#classifications}}
                  <div class="pd-features-desc">
                    {{#specifications}}
                    <div>
                      <div class="pd-feature-details">{{key}}</div>
                      <div class="pd-features-desc">
                        {{value}}
                      </div>
                    </div>
                    {{/specifications}}
                  </div>
                  {{/classifications}}
                </div>
              </section>

              <section>
                <h4 class="no-select">Warranty<span class="pd-features-arrow"></span></h4>
                <div>
                  {{#warranty}}
                  <div class="pd-features-desc">
                    {{.}}
                  </div>
                  {{/warranty}}
                </div>
              </section>
            </amp-accordion>
          </div>

        </div>
      </div>

      <!-- NOTE: Bottm Buttons -->
      <div class="bottom-snackbar-padding">
        &nbsp;
      </div>
      <div class="pd-bottom">
        <a href="/p-{{productListingId}}?saveToListAmp=true"><span class="bottom-left-btn">
          <span><span class="save-icon"></span>Save</span>
        </span></a>
        <a href="/p-{{productListingId}}?addToBagAmp=true"><span class="bottom-right-btn" [class]="{{winningSellerAvailableStock}} > 0 ? 'bottom-right-btn' : 'bottom-right-btn grey-out'">
          <span><span class="bag-icon"></span>Add to bag</span>
        </span></a>
      </div>

    </template>
  </amp-list>
</div>
