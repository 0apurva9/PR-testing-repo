                      {{#dimensionList}}
                        <div>
                          <div class="pd-size-txt">{{dimension}}</div>
                          <div class="pd-size-val"> : {{dimensionValue}}{{dimensionUnit}}</div>
                        </div>
                      {{/dimensionList}}
                    </div>
                  </section>
                  {{/sizeGuideList}}
                </amp-accordion>
              </div>
            </template>
          </amp-list>
        </td>
      </tr>
    </table>
