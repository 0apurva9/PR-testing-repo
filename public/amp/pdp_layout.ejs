          <tr>
            <td><span class="promo-close" on="tap:AMP.setState({showPotentialPromo : false})" role="show-promo" tabindex="1"></span></td>
            <td>
              <p class="promo-heading">Offers</p>
              <div class="promo-body">
                <span>{{potentialPromotions.title}}</span>
                <div class="promo-desc">
                  <span>{{{potentialPromotions.description}}}</span>
                </div>
                <div class="promo-valid">
                  <div class="valid-from">
                    <span>Valid From</span><br />
                    <span>{{potentialPromotions.startDate}}</span>
                  </div>
                  <div class="valid-to">
                    <span>Valid Till</span><br />
                    <span>{{potentialPromotions.endDate}}</span>
                  </div>
                </div>
              </div>
              {{#productOfferMsg}}
              <div class="promo-body">
                <span>{{productOfferMsg.messageID}}</span>
                <div class="promo-desc">
                  <span>{{{productOfferMsg.messageDetails}}}</span>
                </div>
                <div class="promo-valid">
                  <div class="valid-from">
                    <span>Valid From</span><br />
                    <span>{{productOfferMsg.startDate}}</span>
                  </div>
                  <div class="valid-to">
                    <span>Valid Till</span><br />
                    <span>{{productOfferMsg.endDate}}</span>
                  </div>
                </div>
              </div>
              {{/productOfferMsg}}
            </td>
          </tr>
        </table>
      </div>

      <!-- NOTE: EMI Options Popup -->
      <div class="hide-content" [class]="showEMIOptions ? 'promo-overlay' : 'hide-content'">

      </div>
      <div class="hide-content" [class]="showEMIOptions ? 'promo-info' : 'hide-content'">
        <table border="0">
          <tr>
            <td><span class="promo-close" on="tap:AMP.setState({showEMIOptions : false})" role="show-promo" tabindex="1"></span></td>
            <td>
              <p class="emi-heading">EMI details</p>
              <div class="emi-body">
                <span>EMI for the product is provided by the following banks</span>
                <div class="emi-list">

                </div>
                <div class="emi-terms">
                  <span>View Terms & Conditions</span>
                </div>
              </div>
            </td>
          </tr>
        </table>
      </div>

      <!-- NOTE: Jewellery Price Options Popup -->
      <div class="hide-content" [class]="showPriceOptions ? 'promo-overlay' : 'hide-content'">

      </div>
      <div class="hide-content" [class]="showPriceOptions ? 'promo-info' : 'hide-content'">
        <table border="0">
          <tr>
            <td><span class="promo-close" on="tap:AMP.setState({showPriceOptions : false})" role="show-priceoptions" tabindex="1"></span></td>
            <td>
              <p class="emi-heading">Price breakup</p>
              <div class="">
                {{#priceBreakUpDetailsMap}}
                  <div class="price-breakup-detail">
                    <p class="price-br-head">{{name}} <span class="price-br-info">{{price.formattedValue}}</span></p>
                    {{#weightRateList}}
                      <div class="price-br-body">{{.}}</div>
                    {{/weightRateList}}
                  </div>
                {{/priceBreakUpDetailsMap}}
              </div>
            </td>
          </tr>
        </table>
      </div>

      <!-- NOTE: Header Content -->

      <div [class]="checkPincode ? 'background-blr' : ''">
        <div class="header-content">
          <div class="left-back">
            <span></span>
          </div>
          <div class="left-logo">
            <span></span>
          </div>
          <div class="right-save">
            <span></span>
          </div>
          <div class="right-bag">
            <span></span>
          </div>
        </div>
        <div id="headerContentBlack" class="header-content-black">
          <div class="left-back">
            <span></span>
          </div>
          <div class="left-logo">
            <span></span>
          </div>
          <div class="middle-title">
            {{productName}}
          </div>
          <div class="right-save">
            <span></span>
          </div>
          <div class="right-bag">
            <span></span>
          </div>
        </div>
      </div>

      <!-- NOTE: Product Images -->
