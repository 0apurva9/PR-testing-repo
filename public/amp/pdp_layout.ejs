<body>
  <amp-animation id="showFixedHeader" layout="nodisplay">
    <script type="application/json">
      {
        "duration": "200ms",
         "fill": "both",
         "iterations": "1",
         "direction": "alternate",
         "animations": [
           {
             "selector": "#headerContentBlack",
             "keyframes": [
               { "opacity": "1", "visibility": "visible" }
             ]
           }
         ]
      }
    </script>
  </amp-animation>

  <amp-animation id="hideFixedHeader" layout="nodisplay">
     <script type="application/json">
       {
         "duration": "200ms",
           "fill": "both",
           "iterations": "1",
           "direction": "alternate",
           "animations": [
             {
               "selector": "#headerContentBlack",
               "keyframes": [
                 { "opacity": "0", "visibility": "hidden" }
               ]
             }
           ]
       }
     </script>
   </amp-animation>

