        <div class="pd-details">
          <div class="pd-details-1">
            <span class="pd-brandname">{{brandName}}</span>
            <!-- <p [text]="productData.endpoint"></p> -->
            <span class="pd-winning-price">{{winningSellerPrice.formattedValueNoDecimal}}</span>
          </div>
          <div class="pd-details-2">
            <span class="pd-productname">{{productName}}</span>
            <span class="pd-mrp-price" [class]="{{mrpPrice.doubleValue}} != {{winningSellerPrice.doubleValue}} ? 'pd-mrp-price' : 'hide-content'"><span>{{mrpPrice.formattedValueNoDecimal}}</span> <span class="pd-discount">({{discount}}%)</span></span>
          </div>

          <div class="hide-content" [class]="{{averageRating}} ? 'pd-rating' : 'hide-content'">
            <a href="/p-{{productListingId}}/product-reviews">
              <div class="hide-content" [class]="{{averageRating}} && {{averageRating}} >= 1 && {{averageRating}} < 2 ? 'rating-stars' : 'hide-content'">
                <span class="star-fill"></span><span class="star-nofill"></span><span class="star-nofill"></span><span class="star-nofill"></span><span class="star-nofill"></span>
              </div>
              <div class="hide-content" [class]="{{averageRating}} && {{averageRating}} >= 2 && {{averageRating}} < 3 ? 'rating-stars' : 'hide-content'">
                <span class="star-fill"></span><span class="star-fill"></span><span class="star-nofill"></span><span class="star-nofill"></span><span class="star-nofill"></span>
              </div>
              <div class="hide-content" [class]="{{averageRating}} && {{averageRating}} >= 3 && {{averageRating}} < 4 ? 'rating-stars' : 'hide-content'">
                <span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span><span class="star-nofill"></span><span class="star-nofill"></span>
              </div>
              <div class="hide-content" [class]="{{averageRating}} && {{averageRating}} >= 4 && {{averageRating}} < 5 ? 'rating-stars' : 'hide-content'">
                <span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span><span class="star-nofill"></span>
              </div>
              <div class="hide-content" [class]="{{averageRating}} && {{averageRating}} >= 5 ? 'rating-stars' : 'hide-content'">
                <span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span><span class="star-fill"></span>
              </div>
              <span class="rating-text">Rating <span class="rating-value">{{averageRating}}</span>/5</span> <span class="rating-arrow"></span>
            </a>
          </div>
          <div class="hide-content" [class]="'{{rootCategory}}' == 'FineJewellery' ? 'pd-details-4' : 'hide-content'">
            <div class="price-breakup">
              <span on="tap:AMP.setState({showPriceOptions:true})" role="price-breakup" tabindex="0">Price Breakup</span>
            </div>
            {{details.0.value}} <span class="desc-readmore" [class]="descReadMore ? 'hide-content' : 'desc-readmore'" on="tap:AMP.setState({descReadMore : true})" role="read-desc" tabindex="0">Read More</span>
            <br /><p class="hide-content" [class]="descReadMore ? 'show-content' : 'hide-content'">{{productDescription}}</p>
          </div>
          <div class="hide-content" [class]="'{{isEMIEligible}}' == 'Y' ? 'pd-emi' : 'hide-content'">
            <span>EMI avaliable on this product,</span>
            <!-- <span on="tap:AMP.setState({showEMIOptions:true})" role="check-emi" tabindex="0">View Plans</span> -->
            <a href="/p-{{productListingId}}?showAmpEmi=true">View Plans</a>
          </div>
          <div class="hide-content" [class]="'{{isCOD}}' == 'Y' ? 'pd-details-3' : 'hide-content'">
            Cash on Delivery available
          </div>
        </div>
      </div>

      <!-- NOTE: EMI Available -->
      <div [class]="checkPincode ? 'background-blr' : ''">

      </div>

      <!-- NOTE: Promotions -->
      <div [class]="checkPincode ? 'background-blr' : ''">
        {{#potentialPromotions}}
        <div class="potential-promos" on="tap:AMP.setState({showPotentialPromo : true})" role="show-promo" tabindex="0">
          <span>{{potentialPromotions.title}}</span>
          {{#productOfferMsg}}
          <br />
          <span class="product-offer-title">{{productOfferMsg.messageID}}</span>
          {{/productOfferMsg}}
        </div>
        {{/potentialPromotions}}
      </div>

      <!-- NOTE: Variant Options -->
      <div [class]="checkPincode ? 'background-blr' : ''">
        {{#variantOptionsAMP}}
        {{#variantOptionsAMP.variantSizes.0}}
        <div class="variant-head">
          <span>Select Size</span>
          <span on="tap:AMP.setState({sizeGuide:true, productId : '{{productListingId}}'})" role="check-size" tabindex="0">Size guide</span>
        </div>
        <div class="variant-body">
          <amp-selector layout="container" on="select:AMP.setState({selectedVariant : event.targetOption})">
