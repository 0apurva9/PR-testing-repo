/* NOTE: AMP Carousel Btns */
.amp-carousel-button-prev, .amp-carousel-button-next {border-radius: 50%;}

.pd-details-1 {margin-bottom: 10px; color: #181818;}
.pd-details-2 {font-weight: 300; margin-bottom: 10px;}
.pd-details-3 {font-size: 14px; line-height: 22px;}
.pd-details-4 {font-size: 14px; line-height: 22px; margin-bottom: 20px;}
.pd-details {padding: 15px; font-size: 16px; background-color: #fff;}
.pd-rating {font-size: 14px; color: #8d8d8d; line-height: 25px; font-family: regular; margin-bottom: 10px; display: inline-block; }
.pd-rating a {text-decoration: none; -webkit-tap-highlight-color: #fff; color: #8d8d8d;}
.rating-stars {display: inline-block;}
.rating-stars .star-fill {width: 15px; height: 15px; background-image: url('https://pwassets.tatacliq.com/static/media/star-fill.7e6f1450.svg');
  background-position: 50%; display: inline-block; background-size: contain; margin-right: 4px;}
.rating-stars .star-nofill {width: 15px; height: 15px; background-image: url('https://pwassets.tatacliq.com/static/media/star-stroke.bcd1687c.svg');
  background-position: 50%; display: inline-block; background-size: contain; margin-right: 4px;}
.pd-rating .rating-value {max-width: 2.2ch; overflow: hidden; display: inline-block; vertical-align: bottom;}
.rating-arrow {width: 15px; height: 15px; background-image: url('https://pwassets.tatacliq.com/static/media/arrow.813ce519.svg');
