  <amp-list id="productContainerMain" src="<%= pdpAPI %>"
    width="auto" height="600" layout="fixed-height">
    <template type="amp-mustache">

      <div id="pdp-marker">
        <amp-position-observer on="enter:hideFixedHeader.start;exit:showFixedHeader.start" layout="nodisplay">
        </amp-position-observer>
      </div>

      <!-- NOTE: Promotions Popup -->
      <div class="hide-content" [class]="showPotentialPromo ? 'promo-overlay' : 'hide-content'">

      </div>
      <div class="hide-content" [class]="showPotentialPromo ? 'promo-info' : 'hide-content'">
