.price-breakup-detail {padding: 10px 15px; color: #212121; height: auto;}
.price-breakup-detail .price-br-info {float: right;}
.price-br-head {margin: 0; line-height: 22px;}
.price-br-body {line-height: 21px;}
.pd-emi {font-size: 14px; background-color: #fff; line-height: 22px;}
.pd-emi span:last-child, .desc-readmore, .pd-emi a {color: rgb(255, 23, 68); text-decoration: none; -webkit-tap-highlight-color: #fff;}
.promo-overlay {width: 100%; height: 100%; position: absolute; z-index: 100; display: block; filter: blur(10px); background: #ddd;}
.pincode-check {width: 100%; height: auto; padding: 0 130px 0 15px; position: relative; margin: 30px 0 20px; display: inline-flex;}
