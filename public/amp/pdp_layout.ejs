        <td>
          <amp-list src="<%= sizeGuideURL %>" width="auto" height="600" layout="fixed-height">
            <template type="amp-mustache">
              <amp-img class="contain" src="{{imageURL}}" width="380" height="200" layout="responsive"></amp-img>
              <div class="pd-sizes">
                <amp-accordion id="productSizes" animate>
                  {{#sizeGuideList}}
                  <section>
                    <h4 class="no-select">{{dimensionSize}}<span class="pd-features-arrow"></span></h4>
